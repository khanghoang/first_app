<% provide(:title,"All users") %>
<h1>All users</h1>
<ul class="users" id="users">
	<%= render @users %>
</ul>
<%= form_tag(users_path,remote: true, method: :get) do %>
  <%= hidden_field_tag :page , 1%>
  <%= submit_tag "View more", class: "btn btn-large btn-primary", style: "margin-top: 10px" %>
<% end %>